<?xml version="1.0" encoding="UTF-8"?>
<settings xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd" 
	xmlns="http://maven.apache.org/SETTINGS/1.1.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">  
	<servers>
		<server>
			<id>gitlab-maven</id>
			<configuration>
				<httpHeaders>
				<property>
					<name>Job-Token</name>
					<value>${CI_JOB_TOKEN}</value>
				</property>
				</httpHeaders>
			</configuration>
		</server>
		<server>
			<username>gitlab.pipeline</username>
			<password>${ARTIFACTORY_TOKEN_GITLAB_PIPELINE}
</password>
			<id>central</id>
		</server>
		<server>
			<username>gitlab.pipeline</username>
			<password>${ARTIFACTORY_TOKEN_GITLAB_PIPELINE}
</password>
			<id>snapshots</id>
		</server>
		<server>
			<username>gitlab.pipeline</username>
			<password>${ARTIFACTORY_TOKEN_GITLAB_PIPELINE}
</password>
			<id>releases</id>
		</server>
		<server>
			<username>gitlab.pipeline</username>
			<password>${ARTIFACTORY_TOKEN_GITLAB_PIPELINE}
</password>
			<id>ftp-repository</id>
		</server>
	</servers>

	<profiles>
		<profile>
			<id>artifactory</id>
			<repositories>
				<repository>
					<id>snapshots</id>
					<name>libs-snapshot</name>
					<url>https://artifactory.snaitech.net/artifactory/libs-snapshot</url>
					<releases>
						<enabled>false</enabled>
					</releases>
				</repository>
				<repository>
					<id>central</id>
					<name>libs-release</name>
					<url>https://artifactory.snaitech.net/artifactory/libs-release</url>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
				</repository>
			</repositories>
			<pluginRepositories>
				<pluginRepository>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<id>central</id>
					<name>plugins-release</name>
					<url>https://artifactory.snaitech.net/artifactory/libs-release</url>
				</pluginRepository>
				<pluginRepository>
					<snapshots/>
					<releases>
						<enabled>false</enabled>
					</releases>
					<id>snapshots</id>
					<name>plugins-snapshot</name>
					<url>https://artifactory.snaitech.net/artifactory/libs-snapshot</url>
				</pluginRepository>
			</pluginRepositories>
		</profile>
	</profiles>
	<activeProfiles>
		<activeProfile>artifactory</activeProfile>
	</activeProfiles>


</settings>
